{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2021"],
    "types": ["node"],
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "skipLibCheck": true
  }
}
